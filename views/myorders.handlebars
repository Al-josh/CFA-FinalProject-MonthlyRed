<div class="container">
  <h3>My orders</h3>
  {{#each orders}}
    <div class="panel panel-default">
      <div class="panel-body">
        <ul class="list-group">
          {{#each this.items}}
            <li class="list-group-item">
              <span class="badge">{{this.price}}</span>
              {{this.name}} | {{this.qty}}
            </li>
          {{/each}}
        </ul>
      </div>
      <div class="panel-footer">AUD {{this.cart.totalPrice}}</div>
    </div>
  {{/each}}
  </div>
</div>
